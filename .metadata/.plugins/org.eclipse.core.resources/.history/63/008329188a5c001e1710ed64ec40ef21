package pc;
import java.util.*;
public class ques {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        int n=sc.nextInt();
        PriorityQueue<Integer> pq=new PriorityQueue<>();
        int size=0;
        for(int i=0;i<n;i++){
            pq.add(sc.nextInt());
            size++;
            Iterator<Integer> value=pq.iterator();
            int j=0;
            if(size%2==1){
            while(value.hasNext()){
                if(j==size/2){
                    System.out.print(value.next()+" ");
                    
                }
                else
                value.next();
                j++;
            }
            }
            else {
                while(value.hasNext()){
                if(j==(size/2)-1){
//                	System.out.println(size);
                    int a=value.next();
                    int b=value.next();
                    System.out.print((a+b)/2+" ");
                
                }
                else
                value.next();
                j++;
            }
            }
        }
    }
}