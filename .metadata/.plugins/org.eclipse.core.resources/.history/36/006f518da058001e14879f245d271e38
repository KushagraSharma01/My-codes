package pc;
import java.util.*;
public class p2{
    public static void main(String [] args){
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-->0){
            int n=sc.nextInt();
            int w=sc.nextInt();
            int a[]=new int[n];
            int s=0;
            for(int i=0;i<n;i++){
                a[i]=sc.nextInt();
                if(a[i]<)
                s+=a[i];
            }
            int h=(w+s)/n;
            if(h*n<(w+s))
            	h--;
            System.out.println(h);
            
        }
            
    }
}