package Map;
import java.util.*;
//HashMap TreeMap Linked hashMap

public class Map {
	
	public static void main(String []args) {
		HashMap<String,Integer> map=new HashMap<>();
		map.put("Raj",86);
		map.put("Manish", 70);
		map.put("Amisha", 67);
		map.put("Puneet", 80);
		map.put("Ankit", 67);
		map.put("Puneet", 78);
		System.out.println(map);
		//key is unique
		//get O(1)
		System.out.println(map.get("Ankit"));
		System.out.println(map.get("Rohit"));
		//remove O(1)
		System.out.println(map.remove("Ankit"));
		System.out.println(map);
		//contains O(1)
		System.out.println(map.containsKey("Kunal"));
		System.out.println(map.containsKey("Raj"));
		//null as a key is allowed
		
	}
}
