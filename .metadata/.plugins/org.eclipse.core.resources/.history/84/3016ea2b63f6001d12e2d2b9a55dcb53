package Question_Linked_List;
import java.util.ArrayList;
import java.util.*;
public class dumy {
	public static void main (String args[]) {
		ArrayList ll=new ArrayList();
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<n;i++){
			ll.add(sc.nextInt());
		}
		for(int i=0;i<ll.size();i++){
			int x=ll.get(i);
			if(x%2==0){
				ll.remove(i);
				ll.add(x);
			}
		}
		for(int i=0;i<ll.size();i++){
			System.out.print(ll.get(i)+" ");
		}
    }

}
